{
  "name": "zoom-license-manager",
  "version": "1.0.0",
  "description": "Dashboard for managing Zoom licenses and Moodle accounts",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm run dev",
    "install:all": "npm install && cd backend && npm install && cd ../frontend && npm install"
  },
  "keywords": [
    "zoom",
    "moodle",
    "license-management"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/nodemailer": "^7.0.10",
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "nodemailer": "^8.0.1"
  }
}
